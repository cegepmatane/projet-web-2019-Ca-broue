RewriteEngine on
RewriteRule ^$ publique/vue/index.php
RewriteRule ^accueil publique/vue/index.php
RewriteRule ^boutique/goodie/(\d+) publique/vue/goodie.php?id=$1
RewriteRule ^boutique/ajouter-panier/(\d+) publique/vue/ajouter-panier.php?id=$1
RewriteRule ^boutique publique/vue/liste-goodies.php
RewriteRule ^biere publique/vue/biere.php
RewriteRule ^inscription publique/vue/inscription.php
RewriteRule ^panier publique/vue/panier.php
RewriteRule ^connexion commun/vue/connexion.php
RewriteRule ^contact publique/vue/contact.php
RewriteRule ^a-propos publique/vue/a-propos.php
RewriteRule ^mon-compte utilisateur/vue/mon-compte.php
RewriteRule ^modifier-compte utilisateur/vue/modifier-info.php
RewriteRule ^goodie-image/goodie_(\d+).jpeg publique/illustration/goodie-image/goodie_$1.jpeg
RewriteRule ^conditions publique/vue/condition-confidentialite.php


RewriteRule ^utilisateur/paiement utilisateur/vue/paiement.php

RewriteRule ^admin$ administration/vue/index.php
RewriteRule ^admin/$ administration/vue/index.php
RewriteRule ^admin/accueil$ administration/vue/index.php
RewriteRule ^admin/goodies$ administration/vue/liste-goodies.php
RewriteRule ^admin/goodie/ajouter$ administration/vue/ajouter-goodie.php
RewriteRule ^admin/goodie/modifier/(\d+)$ administration/vue/modifier-goodie.php?id=$1
RewriteRule ^admin/categories$ administration/vue/liste-categories.php
RewriteRule ^admin/categorie/ajouter$ administration/vue/ajouter-categorie.php
RewriteRule ^admin/categorie/modifier/(\d+)$ administration/vue/modifier-categorie.php?id=$1
RewriteRule ^admin/utilisateurs$ administration/vue/liste-utilisateurs.php
RewriteRule ^admin/bieres$ administration/vue/liste-bieres.php
RewriteRule ^admin/biere/ajouter$ administration/vue/ajouter-biere.php
RewriteRule ^admin/biere/modifier/(\d+)$ administration/vue/modifier-biere.php?id=$1
RewriteRule ^admin/recherche-achat$ administration/vue/recherche-achat.php
RewriteRule ^admin/historique/datetransaction/(([12]\d{3}-(0[1-9]|1[0-2])-(0[1-9]|[12]\d|3[01]))) administration/vue/historique-des-transactions.php?datetransaction=$1
RewriteRule ^admin/historique$ administration/vue/historique-des-transactions.php
RewriteRule ^admin/calendrier$ administration/vue/calendrier.php


